datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id          String  @id @default(auto()) @map("_id") @db.ObjectId
    cpf         Int     @unique
    email       String
    name        String?
    phoneNumber Int
    address     String
}
